SELECTED_EMOJI=${EMOJIS[$RANDOM % ${#EMOJIS[@]}]};

POWERLEVEL9K_CUSTOM_RANDOM_WORD="zsh_random_word"
zsh_random_word(){
    local RWORD=$(gshuf -n 1 /usr/share/dict/words)
    declare -g RWORD="$RWORD"
    echo -n "rw:$RWORD"
}
define(){
    echo -n "$RWORD"
}
POWERLEVEL9K_CUSTOM_RANDOM_WORD_BACKGROUND="clear"
POWERLEVEL9K_CUSTOM_RANDOM_WORD_FOREGROUND="cyan"
POWERLEVEL9K_CUSTOM_RANDOM_WORD_ICON="\ufb1c"

